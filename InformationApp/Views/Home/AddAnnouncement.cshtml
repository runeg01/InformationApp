@model InformationApp.Models.Announcements
@{
    ViewBag.Title = "AddAnnouncement";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>AddAnnouncement</h2>
<div>
    <p>Member Name:@Html.TextBoxFor(x => x.AnnouncementName)</p>
    <p>Member Email:@Html.TextBoxFor(x => x.AnnouncementDescription)</p>
    <p>Member Address:@Html.TextBoxFor(x => x.AnnouncementAuthor)</p>
   
    

    <input type="Submit" value="Submit Announcement"
</div>
